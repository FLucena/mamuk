{
  "name": "mamuk-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:server && npm run build:client",
    "build:client": "tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json && npm run copy-server-files && node convert-models-to-cjs.cjs",
    "copy-server-files": "node -e \"if (require('fs').existsSync('dist-server/server.js')) { require('fs').copyFileSync('src/server-package.json', 'dist-server/package.json'); require('fs').copyFileSync('dist-server/server.js', 'dist-server/server.cjs'); }\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "analyze": "source-map-explorer 'dist/assets/*.js'",
    "server": "node dist-server/server.cjs",
    "server:dev": "nodemon --watch src --ext ts,js,json --ignore src/**/*.spec.ts --exec \"npm run build:server && node dist-server/server.cjs\"",
    "start": "NODE_ENV=production node dist-server/server.cjs",
    "test": "jest",
    "test:unit": "jest src/tests/unit",
    "test:integration": "jest src/tests/integration",
    "test:mongodb": "ts-node src/tests/db-test.ts",
    "test:mongodb:crud": "ts-node src/tests/db-crud-test.ts",
    "install:all": "npm install",
    "dev:all": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "postinstall": "node --eval \"try{require('fs').unlinkSync('./node_modules/.bin/ts-node')}catch(e){}\""
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "framer-motion": "^12.6.0",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.1",
    "lucide-react": "^0.484.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.4.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.16",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.5.9",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/supertest": "^2.0.16",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-styled-components": "^2.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "glob": "^10.3.10",
    "globals": "^15.15.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.3",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.14.0",
    "superagent": "^8.1.2",
    "supertest": "^6.3.3",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "vite": "^6.2.3"
  },
  "description": "This template provides a minimal setup to get React working in Vite with HMR and some ESLint rules.",
  "main": ".eslintrc.cjs",
  "keywords": [],
